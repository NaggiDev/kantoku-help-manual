{
  "name": "kantoku-help-manual",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t kantoku-help-manual:latest .",
    "docker:run": "docker run -p 3000:3000 kantoku-help-manual:latest",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker system prune -f && docker image prune -f",
    "jenkins:setup": "chmod +x jenkins/setup/jenkins-setup.sh && ./jenkins/setup/jenkins-setup.sh",
    "jenkins:validate": "jenkins-cli validate-jenkinsfile < Jenkinsfile",
    "jenkins:build": "jenkins-cli build kantoku-help-manual",
    "jenkins:logs": "jenkins-cli console kantoku-help-manual"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "15.3.3",
    "next-intl": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
